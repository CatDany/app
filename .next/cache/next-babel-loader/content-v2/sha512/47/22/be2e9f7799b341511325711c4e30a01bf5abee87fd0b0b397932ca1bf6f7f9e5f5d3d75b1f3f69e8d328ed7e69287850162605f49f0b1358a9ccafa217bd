\n            days = [...days, i];\n          this.setState({ prices, days, cash, shares, pricePerShare });\n        });\n    });\n    \n  }\n  \n  componentWillUnmount() {\n    this.pusher.disconnect();\n  }\n  \n  render() {\n    return (\n      <Layout pageTitle=\"Realtime Data Visualization\">\n        <div id=\"sliderFade\" class=\"carousel slide carousel-fade\" data-ride=\"carousel\">\n          <ol class=\"carousel-indicators\">\n            <li data-target=\"#sliderFade\" data-slide-to=\"0\" class=\"active\"></li>\n            <li data-target=\"#sliderFade\" data-slide-to=\"1\"></li>\n            <li data-target=\"#sliderFade\" data-slide-to=\"2\"></li>\n          </ol>\n          <div class=\"carousel-inner\" >\n            <div class=\"carousel-item active\" style={{height: '100vh'}}>\n              <img src=\"img/slider/bg1.jpg\" class=\"d-block w-100\" />\n              <div class=\"carousel-caption d-none d-md-block\">\n                <h1>TEXT TEXT TEXT</h1>\n                <p>text text text text</p>\n                <a href=\"#\" class=\"button\">\n                  <span class=\"button__line button__line--top\"></span>\n                  <span class=\"button__line button__line--right\"></span>\n                  <span class=\"button__line button__line--bottom\"></span>\n                  <span class=\"button__line button__line--left\"></span>\n                  Начать\n                </a>\n              </div>\n              <h2></h2>\n            </div>\n            <div class=\"carousel-item\" style={{height: '100vh'}}>\n              <img src=\"img/slider/bg2.jpg\" class=\"d-block w-100\" />\n              <div class=\"carousel-caption d-none d-md-block\">\n                <h1>TEXT TEXT TEXT222</h1>\n                <p>text text text text</p>\n              </div>\n            </div>\n            <div class=\"carousel-item\" style={{height: '100vh'}}>\n              <img src=\"img/slider/bg3.jpg\" class=\"d-block w-100\" />\n              <div c